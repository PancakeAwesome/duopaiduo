                            <i class="icon-star"></i>
                            <i class="icon-star"></i>
                            <i class="icon-star"></i>
                            <i class="icon-star"></i>
                        </div>
                    </div>
                </div>
            </div>

            <div class="clearfix">
                <textarea  class="thewidth100 lis_bo1" onfocus="if(value=='评论') {value=''}" onblur="if(value=='') {value='评论'}">评论</textarea>
            </div>
        </div>
        

        <div class="tijiao clearfix ta_c ma_t1">
           <button class="btn red_bg wb50">提交</button> 
        </div>
        
    </div>
<!--/wrap-->
</div>
<script>
    $('.xin').find('i').click(function(){
        $('.xin').find('i').each(function(){
            $(this).removeClass('red');
        });
        $(this).addClass('red').prevAll('i').addClass('red');
    });
</script>
