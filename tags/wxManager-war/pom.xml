	<profiles>
		<profile>
			<id>dev</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<profile.path>env/dev</profile.path>
			</properties>
		</profile>
		<profile>
			<id>test</id>
			<properties>
				<profile.path>env/test</profile.path>
			</properties>
		</profile>
		<profile>
			<id>pre</id>
			<properties>
				<profile.path>env/pre</profile.path>
			</properties>
		</profile>
		<profile>
			<id>product</id>
			<properties>
				<profile.path>env/product</profile.path>
			</properties>
		</profile>
	</profiles>

	<build>
		<finalName>wxManager-war</finalName>

		<resources>
			<resource>
				<directory>${profile.path}</directory>
				<filtering>true</filtering>
			</resource>
			<resource>
				<directory>src/main/resources</directory>
			</resource>
			<resource>
				<directory>src/main/java</directory>
				<includes>
					<include>**/*.xml</include>
					<include>**/*.properties</include>
					<include>**/*.tld</include>
				</includes>
			</resource>
		</resources>

		<plugins>
			<plugin>
				<groupId>org.apache.tomcat.maven</groupId>
				<artifactId>tomcat7-maven-plugin</artifactId>
				<configuration>
					<port>8080</port>
					<uriEncoding>UTF-8</uriEncoding>
					<path>/platform</path>
					<httpsPort>8443</httpsPort>
				</configuration>
			</plugin>

			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<configuration>
					<failOnMissingWebXml>false</failOnMissingWebXml>
				</configuration>
			</plugin>

		</plugins>


	</build>
